#include "allvars.h"

void read_parameters(char configfile[MAXSTRING])
{
  FILE* fp;
  int i, nt;
  void *addr[MAXTAGS];
  int id[MAXTAGS];
  char tag[MAXTAGS][128];
  nt = 0;

  strcpy(tag[nt], "Halofolder");
  addr[nt++] = Foldername;

  strcpy(tag[nt], "FilePrefix");
  addr[nt++] = FilePrefix;

  strcpy(tag[nt], "SnapTimeFile");
  addr[nt++] = SnapTimeFile;
  
  fp = fopen(configfile,"r");
  fclose(fp);
}
